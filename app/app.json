{
  "expo": {
    "name": "Flow",
    "slug": "Flow",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lerry.flow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-updates",
        {
          "username": "lerry"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "760cc873-ebac-41f1-b467-1883ca926a0f"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/760cc873-ebac-41f1-b467-1883ca926a0f"
    },
    "owner": "lerry",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
